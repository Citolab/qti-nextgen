    "diff-dom": "5.0.7",
    "haunted": "^5.0.0",
    "html-to-image": "^1.11.11",
    "lit": "^3.2.0",
    "lit-xml": "^0.6.2",
    "log4javascript": "^1.4.16",
    "modern-normalize": "^3.0.1",
    "rangy2": "^2.1.0",
    "remark-gfm": "^4.0.0",
    "shadow-selection-polyfill": "^1.1.0",
    "typechat": "^0.1.1",
    "xml-formatter": "^3.6.3"
  },
  "peerdependencies": {
    "@lit-labs/context": "^0.3.0",
    "cheerio": "^1.0.0-rc.12",
    "haunted": ">= 5 < 6",
    "lit": ">= 2 < 3"
  },
