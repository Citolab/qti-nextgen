    "add": "^2.0.6",
    "autoprefixer": "^10.4.20",
    "cem-plugin-vs-code-custom-data-generator": "^1.4.2",
    "esbuild-sass-plugin": "^3.3.1",
    "eslint": "^9.21.0",
